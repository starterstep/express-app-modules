}
".callout": {
	backgroundColor: Alloy.Globals.foundation.defaults.color.lightestGray,
	height: Ti.UI.SIZE
}
".callout-content": {
	left: Alloy.Globals.foundation.defaults.position.medium,
	top: Alloy.Globals.foundation.defaults.position.medium,
	right: Alloy.Globals.foundation.defaults.position.medium,
	height: Ti.UI.SIZE
}

".no-select[platform=ios]": {
