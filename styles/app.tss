// Views
"Window": {
	backgroundColor: Alloy.Globals.foundation.defaults.color.white
}
"View": {
	layout: 'vertical',
	height: Ti.UI.SIZE
}
"ScrollView": {
	layout: 'vertical',
	contentWidth:Ti.UI.FILL
}
"WebView": {
	width: "100%" //fixes font changing size
}
"Label": {
	font: {
        fontFamily: Alloy.Globals.foundation.defaults.primary.font.fontFamily,
        fontSize: Alloy.Globals.foundation.defaults.primary.font.fontSize
    },
	color: Alloy.Globals.foundation.defaults.color.black
}
"Button": {
	height: Alloy.Globals.foundation.defaults.height.medium,
	font: {
		fontFamily: Alloy.Globals.foundation.defaults.primary.font.fontFamily,
		fontSize: Alloy.Globals.foundation.defaults.primary.font.fontSize
	},
	width: Ti.UI.FILL,
	backgroundColor: Alloy.Globals.foundation.defaults.color.primary,
	backgroundDisabledColor: Alloy.Globals.foundation.defaults.color.lightGray,
	disabledColor: Alloy.Globals.foundation.defaults.color.lightestGray,
	color: Alloy.Globals.foundation.defaults.color.white,
	selectedBackgroundColor: 'transparent',
	backgroundSelectedColor: 'transparent'
}
"TableView": {
	backgroundColor: 'transparent',
	width: '100%',
	height: Ti.UI.FILL,
	separatorColor: Alloy.Globals.foundation.defaults.color.gray
}
"TableViewRow": {
	font: {
		fontFamily: Alloy.Globals.foundation.defaults.primary.font.fontFamily,
		fontSize: Alloy.Globals.foundation.defaults.primary.font.fontSize
	},
	selectedBackgroundColor: Alloy.Globals.foundation.defaults.color.lightestGray,
	backgroundSelectedColor: Alloy.Globals.foundation.defaults.color.lightestGray
}
"TextField": {
	height: Alloy.Globals.foundation.defaults.height.medium,
	color: Alloy.Globals.foundation.defaults.color.darkestGray,
	hintTextColor: Alloy.Globals.foundation.defaults.color.gray,
	width: Ti.UI.FILL,
	paddingLeft: Alloy.Globals.foundation.defaults.position.little,
	font: Alloy.Globals.foundation.defaults.primary.font,
	autocorrect: false,
    backgroundColor: Alloy.Globals.foundation.defaults.color.white
}
"TextArea": {
	height: Alloy.Globals.foundation.defaults.height.large,
	color: Alloy.Globals.foundation.defaults.color.darkestGray,
	hintTextColor: Alloy.Globals.foundation.defaults.color.gray,
	width: Ti.UI.FILL,
	autocorrect: false,
	backgroundColor: Alloy.Globals.foundation.defaults.color.white,
	paddingLeft: Alloy.Globals.foundation.defaults.position.little,
	paddingRight: Alloy.Globals.foundation.defaults.position.little,
	font: Alloy.Globals.foundation.defaults.primary.font,
	enableReturnKey: true,
	suppressReturn: false
}

//Loading Indicator
".loading-indicator": {
	layout: 'absolute',
	top: 0,
	left: 0,
	zIndex: 999,
	visible: false,
	height: Ti.UI.FILL
}
".loading-indicator-button": {
	color: Alloy.Globals.foundation.defaults.color.white,
	backgroundColor: Alloy.Globals.foundation.defaults.color.black,
	width: 100,
	height: 40,
	title: 'Loading...'
}

//Tabs
".tabs": {
	layout: 'horizontal',
	horizontalWrap: false,
	backgroundColor: Alloy.Globals.foundation.defaults.color.lightGray
}
".tabs-label": {
    height: Ti.UI.FILL,
    color: Alloy.Globals.foundation.defaults.color.black,
	backgroundColor: Alloy.Globals.foundation.defaults.color.lightGray,
    selectedBackgroundColor: Alloy.Globals.foundation.defaults.color.lightestGray,
    backgroundSelectedColor: Alloy.Globals.foundation.defaults.color.lightestGray,
    selectedColor: Alloy.Globals.foundation.defaults.color.black,
	textAlign: Ti.UI.TEXT_ALIGNMENT_CENTER,
	wordWrap: false
}

//Image Select
".image-select": {
	width: 128,
	height: 128,
	hires: true
}

//Icon Bar
".icon-bar-container": {
	layout: 'horizontal',
	horizontalWrap: false,
	backgroundColor: Alloy.Globals.foundation.defaults.color.lightGray
}
".icon-bar-item": {
	backgroundColor: Alloy.Globals.foundation.defaults.color.lightGray,
	selectedBackgroundColor: Alloy.Globals.foundation.defaults.color.lightestGray,
    backgroundSelectedColor: Alloy.Globals.foundation.defaults.color.lightestGray,
    height: Ti.UI.FILL,
	width: Ti.UI.SIZE
}
".icon-bar-icon": {
    height: Ti.UI.FILL,
	color: Alloy.Globals.foundation.defaults.color.black,
	font: {
		fontSize: Alloy.Globals.foundation.defaults.fontSize.big
	}
}
".icon-bar-title": {
	color: Alloy.Globals.foundation.defaults.color.black,
	textAlign: Ti.UI.TEXT_ALIGNMENT_CENTER,
	font: {
		fontSize: Alloy.Globals.foundation.defaults.fontSize.tiny
	}
}

//Sub Nav
".sub-nav-container": {
	layout: 'horizontal',
	backgroundColor: Alloy.Globals.foundation.defaults.color.lightGray,
	height: Alloy.Globals.foundation.defaults.height.medium,
	horizontalWrap: false
}
".sub-nav-item": {
	layout: 'absolute',
	width: Ti.UI.SIZE,
	top: Alloy.Globals.foundation.defaults.position.small,
	bottom: Alloy.Globals.foundation.defaults.position.small,
	left: Alloy.Globals.foundation.defaults.position.small,
    selectedBackgroundColor: Alloy.Globals.foundation.defaults.color.primary,
    backgroundSelectedColor: Alloy.Globals.foundation.defaults.color.primary
}
".sub-nav-title": {
	color: Alloy.Globals.foundation.defaults.color.black,
	wordWrap: false
}

//Top Bar
".top-bar": {
	backgroundColor: Alloy.Globals.foundation.defaults.color.lightestGray,
	height: 45,
	layout: 'absolute'
}
".top-bar-offset": {
	top: 45
}
".top-bar-left": {
	left: 0,
	width: '20%'
}
".top-bar-center": {
	left: '20%',
	right: '20%'
}
".top-bar-right": {
	right: 0,
	width: '20%'
}
".top-bar-label": {
	height: Ti.UI.FILL,
	color: Alloy.Globals.foundation.defaults.color.black,
	font: {
		fontSize: Alloy.Globals.foundation.defaults.fontSize.large
	}
}
".top-bar-button": {
	height: Ti.UI.FILL,
	backgroundColor: 'transparent',
	color: Alloy.Globals.foundation.defaults.color.primary
}

//Bottom Bar
".bottom-bar": {
	bottom: 0,
	height: Alloy.Globals.foundation.defaults.height.large,
	layout: 'absolute'
}

//Progress Bar
".progress": {
	left: 0,
	layout: 'absolute',
	height: Alloy.Globals.foundation.defaults.height.medium,
	backgroundColor: Alloy.Globals.foundation.defaults.color.lightGray
}
".meter": {
	layout: 'absolute',
	width: 0,
	left: 0,
	backgroundColor: Alloy.Globals.foundation.defaults.color.primary
}

".thead": {
	height: Alloy.Globals.foundation.defaults.height.small
}
".th": {
	height: Alloy.Globals.foundation.defaults.height.small,
	font: {
		fontSize: Alloy.Globals.foundation.defaults.fontSize.small,
		fontWeight: 'bold'
	},
	backgroundColor: Alloy.Globals.foundation.defaults.color.lightestGray,
	left: 1,
	right: 1,
	textAlign: Ti.UI.TEXT_ALIGNMENT_CENTER
}
".hr": {
	height: 1,
	width: Ti.UI.FILL,
	backgroundColor: Alloy.Globals.foundation.defaults.color.gray
}
".vr": {
	width: 0.5,
	height: Ti.UI.FILL,
	backgroundColor: Alloy.Globals.foundation.defaults.color.lightGray,
